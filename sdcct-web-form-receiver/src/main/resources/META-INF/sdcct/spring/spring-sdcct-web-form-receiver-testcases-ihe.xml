<?xml version="1.0" encoding="UTF-8"?>
<beans:beans
    xmlns:beans="http://www.springframework.org/schema/beans"
    xmlns:c="http://www.springframework.org/schema/c"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:sdcct="urn:sdcct:spring"
    xmlns:sdcct-xml="urn:sdcct:spring:xml"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        urn:sdcct:spring urn:sdcct:spring:sdcct-spring.xsd
        urn:sdcct:spring:xml urn:sdcct:spring:xml:sdcct-spring-xml.xsd">

    <!--====================================================================================================
    = IHE FORM RECEIVER TESTCASE PROCESSOR
    =====================================================================================================-->
    <beans:bean id="iheFormReceiverTestcaseProcessor" class="gov.hhs.onc.sdcct.web.form.receiver.testcases.ihe.IheFormReceiverTestcaseProcessor"
        parent="iheTestcaseProcessor" abstract="true"/>

    <beans:bean id="iheFormReceiverTestcaseProcessorImpl" class="gov.hhs.onc.sdcct.web.form.receiver.testcases.ihe.impl.IheFormReceiverTestcaseProcessorImpl"
        parent="iheFormReceiverTestcaseProcessor">
        <beans:constructor-arg name="clientBeanName">
            <beans:idref bean="clientFormReceiverRfd"/>
        </beans:constructor-arg>
    </beans:bean>
</beans:beans>