<?xml version="1.0" encoding="UTF-8"?>
<beans:beans
    xmlns:beans="http://www.springframework.org/schema/beans"
    xmlns:c="http://www.springframework.org/schema/c"
    xmlns:p="http://www.springframework.org/schema/p"
    xmlns:sdcct="urn:sdcct:spring"
    xmlns:sdcct-xml="urn:sdcct:spring:xml"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        urn:sdcct:spring urn:sdcct:spring:sdcct-spring.xsd
        urn:sdcct:spring:xml urn:sdcct:spring:xml:sdcct-spring-xml.xsd">
    
    <!--====================================================================================================
    = LISTENER REGISTRATION BEANS
    =====================================================================================================-->
    <beans:bean id="regBeanListener" class="org.springframework.boot.web.servlet.ServletListenerRegistrationBean" abstract="true"/>
    
    <!--====================================================================================================
    = FILTER REGISTRATION BEANS
    =====================================================================================================-->
    <beans:bean id="regBeanFilter" class="org.springframework.boot.web.servlet.FilterRegistrationBean" abstract="true"/>
    
    <!--====================================================================================================
    = SERVLET REGISTRATION BEANS
    =====================================================================================================-->
    <beans:bean id="regBeanServlet" class="org.springframework.boot.web.servlet.ServletRegistrationBean" abstract="true"/>
</beans:beans>